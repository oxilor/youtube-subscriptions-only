{
  "manifest_version": 3,
  "name": "YouTube Subscriptions Only",
  "description": "Shows YouTube videos only from your subscriptions.",
  "version": "1.2",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
